{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coffeestained/spa-personal-shared-service.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "spa-personal-shared-service",
  "author": "Matthew Grady",
  "module": "dist/spa-personal-shared-service.esm.js",
  "size-limit": [
    {
      "path": "dist/spa-personal-shared-service.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/spa-personal-shared-service.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/uuid": "^9.0.7",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/coffeestained/spa-personal-shared-service/issues"
  },
  "homepage": "https://github.com/coffeestained/spa-personal-shared-service#readme",
  "dependencies": {
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  }
}
